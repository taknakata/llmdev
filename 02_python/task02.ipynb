   "source": [
    "price = int(input(\"税抜価格を入力してください: \"))\n",
    "\n",
    "# 税込価格\n",
    "total_price = int(price * 1.1)\n",
    "\n",
    "# 送料判定\n",
    "if total_price >= 2000:\n",
    "    print(\"送料は無料です\")\n",
    "else:\n",
    "    print(\"送料は350円です\")\n",
    "    total_price += 350\n",
    "\n",
    "print(f\"送料込税込価格は{total_price}円です\")\n"
   ]
  }
